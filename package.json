  "eslintConfig": {
    "extends": [
      "preact",
      "prettier"
    ]
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "eslint": "^7.20.0",
    "eslint-config-preact": "^1.1.3",
    "eslint-config-prettier": "^8.1.0",
    "preact-cli": "^3.0.0",
    "prettier": "2.2.1",
    "sirv-cli": "^1.0.3",
    "stylelint": "^13.11.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0"
  },
  "dependencies": {
    "preact": "^10.1.0",
    "preact-render-to-string": "^5.1.2"
  }
}
