{
  "private": true,
  "name": "fakeos",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "preact build",
    "serve": "sirv build --port 8080 --cors --single",
    "dev": "preact watch",
    "lint": "eslint src",
    "stylelint": "npx stylelint '**/*.css'",
    "stylelint-fix": "npx stylelint --fix '**/*.css'",
    "format": "prettier --write ."
